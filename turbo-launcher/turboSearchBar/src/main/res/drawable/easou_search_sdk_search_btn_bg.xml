<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
	    <shape
	    	android:shape="rectangle" >
	    <solid android:color="@color/easou_search_sdk_search_btn_normal" />
	    <corners
	        android:topLeftRadius="@dimen/easou_search_sdk_radius_top_left"
	        android:topRightRadius="@dimen/easou_search_sdk_radius"
	        android:bottomLeftRadius="@dimen/easou_search_sdk_radius_left_bottom"
	        android:bottomRightRadius="@dimen/easou_search_sdk_radius_right_bottom" />
		</shape>

    </item>
    <item android:state_pressed="true" >
        <shape
	    	android:shape="rectangle" >
	    <solid android:color="@color/easou_search_sdk_search_btn_press" />
	    <corners
	        android:topLeftRadius="@dimen/easou_search_sdk_radius_top_left"
	        android:topRightRadius="@dimen/easou_search_sdk_radius"
	        android:bottomLeftRadius="@dimen/easou_search_sdk_radius_left_bottom"
	        android:bottomRightRadius="@dimen/easou_search_sdk_radius_right_bottom" />
		</shape>
        
    </item>
</selector>
